#include <stdio.h>

// Función que devuelve el mayor de tres números
int mayor(int a, int b, int c) {
	if (a > b && a > c)
		return a;
	else if (b > c)
		return b;
	else
		return c;
}

int main() {
	int n1, n2, n3;
	
	printf("Ingrese tres numeros diferentes: ");
	scanf("%d %d %d", &n1, &n2, &n3);
	
	printf("El mayor es: %d\n", mayor(n1, n2, n3));
	
	return 0;
}

